(define s (WordSenseNode "so%4:02:10::")) (PartOfSpeechLink s adv)
(define x (WordNode "therefore")) (WordSenseLink x s)
(define x (WordNode "hence")) (WordSenseLink x s)
(define x (WordNode "thence")) (WordSenseLink x s)
(define x (WordNode "thus")) (WordSenseLink x s)
(define x (WordNode "so")) (WordSenseLink x s)
