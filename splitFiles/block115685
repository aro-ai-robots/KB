(define s (WordSenseNode "often%4:02:00::")) (PartOfSpeechLink s adv)
(define x (WordNode "frequently")) (WordSenseLink x s)
(define x (WordNode "often")) (WordSenseLink x s)
(define x (WordNode "oftentimes")) (WordSenseLink x s)
(define x (WordNode "oft")) (WordSenseLink x s)
(define x (WordNode "ofttimes")) (WordSenseLink x s)
